# Ramp API Configuration
# Leave empty or use "demo" for demo mode with mock data
RAMP_API_KEY=

# NextAuth Configuration
# Use any random string for demo purposes
NEXTAUTH_SECRET=demo-secret-change-in-production
NEXTAUTH_URL=http://localhost:3000

# For production with real Ramp API:
# RAMP_API_KEY=ramp_live_your_actual_api_key_here
# NEXTAUTH_SECRET=your_secure_random_secret_here

# Optional: Database URL for session storage
# DATABASE_URL=postgresql://username:password@localhost:5432/ramp_dashboard

# Optional: SSO Configuration
# OKTA_CLIENT_ID=your_okta_client_id
# OKTA_CLIENT_SECRET=your_okta_client_secret
# OKTA_DOMAIN=your_okta_domain

# AZURE_AD_CLIENT_ID=your_azure_client_id
# AZURE_AD_CLIENT_SECRET=your_azure_client_secret
# AZURE_AD_TENANT_ID=your_azure_tenant_id